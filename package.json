{
  "name": "mcp-server",
  "module": "src/index.ts",
  "type": "module",
  "version": "1.0.0",
  "description": "Model Context Protocol (MCP) Server",
  "private": true,
  "scripts": {
    "start": "npx tsx src/server/http-server.ts",
    "build": "npx build src/index.ts --outdir build --target node",
    "build:http": "npx build src/server/http-server.ts --outdir build --target node",
    "dev": "npx --watch src/index.ts",
    "start:http": "npx tsx src/server/http-server.ts",
    "dev:http": "npx --watch src/server/http-server.ts"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.11.0"
  },
  "peerDependencies": {
    "@valibot/to-json-schema": "^1.0.0",
    "effect": "^3.14.4",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "fastmcp": "^1.21.0",
    "instagram-private-api": "^1.46.1",
    "zod": "^3.24.2"
  }
}
